
const palindromo = 'Ana';
        if (palindromo.toLowerCase[0] == palindromo.toLowerCase[2] && palindromo.toLowerCase[2] ==
            palindromo.toLowerCase[0] && palindromo.toLowerCase[1] == palindromo.toLowerCase[1]) {
           
